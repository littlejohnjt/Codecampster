@page
@model RegistrationSuccessfulModel
@{
    ViewData["Title"] = "Registration Successful";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
        <div>
            <p>
                You have successfully registered for @Model.Event.EventTitle.  You can
                click on your username above to update your profile information.
            </p>
        </div>
        <button type="submit" class="btn btn-default">Continue</button>
    </form>
</div>
